[Plugin]
Module=contacts
Name=Contacts
Description=Sync contacts
Icon=valent-contacts-plugin-symbolic
Builtin=true
Embedded=valent_contacts_plugin_register_types
Website=https://github.com/andyholmes/valent
Help=https://github.com/andyholmes/valent
