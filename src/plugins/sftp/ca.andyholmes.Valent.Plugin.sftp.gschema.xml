<schemalist gettext-domain="valent">
  <schema id="ca.andyholmes.Valent.Plugin.sftp">
    <key name="auto-mount" type="b">
      <default>false</default>
    </key>
    <key name="local-allow" type="b">
      <default>false</default>
    </key>
    <key name="local-port" type="u">
      <default>22</default>
    </key>
  </schema>
</schemalist>
