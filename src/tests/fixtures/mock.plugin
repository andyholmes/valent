[Plugin]
Module=mock
Name=Mock
Description=Mock Plugin
Icon=application-x-addon-symbolic
Builtin=true
Embedded=valent_mock_plugin_register_types
Website=https://github.com/andyholmes/valent
Help=https://github.com/andyholmes/valent
Hidden=false
X-IncomingCapabilities=kdeconnect.mock.echo;kdeconnect.mock.transfer
X-OutgoingCapabilities=kdeconnect.mock.echo;kdeconnect.mock.transfer
# The mock plugins should be lowest priority
X-ClipboardAdapterPriority=1000000
X-ContactStoreProviderPriority=1000000
X-InputAdapterPriority=1000000
X-MediaPlayerProviderPriority=1000000
X-MixerControlPriority=1000000
X-NotificationSourcePriority=1000000
X-SessionAdapterPriority=1000000
